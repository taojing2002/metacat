   <tr class="sectheader"><td colspan="6" align="left"></td></tr>
   <tr class="sectheader"><td colspan="6" align="center" class="borderbottom"><input type="SUBMIT" onClick="sortInputTags()" value="Submit Data Set Description" name="SUBMIT">
              
    </table>
