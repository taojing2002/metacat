      <td/>
      <td align="left"><span class="label">Stop Date</span></td>
      <td colspan="2"></td>
      </tr>
     
      <tr class="sectbody"> 
        <td class="rightCol"> 
          [% IF required.temporal == 'true' %]
            <span class="label">*Year (yyyy)</span>
          [% ELSE %]
    <span class="label">Year (yyyy)</span>
    [% END %]</td>
