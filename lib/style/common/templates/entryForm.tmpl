                <input name="keyword" type="hidden" value="[%$keyword%]"/>
                <input name="keywordType" type="hidden" value="[%$keywordType%]"/>
                <input name="keywordTh" type="hidden" value="[%$keywordTh%]"/>
            </td>
        </tr>
        [% cnt = cnt + 1 %]
       [% END %]
        <tr class="sectbody" id="addkeyrow"><td colspan=6 /></tr>
       </table>
  [% END %]
  
